{
  "name": "tserr-vscode",
  "publisher": "typeholes",
  "version": "0.0.1",
  "main": "./index.js",
  "repository": "https://github.com/typeholes/tserr",
  "license": "MIT",
  "engines": {
    "vscode": "^1.44.0"
  },
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "configuration": {
      "title": "Use Vscode Diagnostics",
      "properties": {
        "TsErr.useVscodeDiagnostics": {
          "default": true,
          "type": "boolean",
          "description": "Report diagnostics from Vscode in the TsErr Problems View"
        }
      }
    },
    "commands": [
      {
        "command": "tserr-problems-view.openExternal",
        "title": "TsErr open external"
      },
      {
        "command": "tserr-problems-view.loadPlugins",
        "title": "TsErr load plugins"
      }
    ],
    "views": {
      "explorer": [
        {
          "type": "webview",
          "id": "TsErr.problems",
          "name": "TsErr Problems View",
          "contextualTitle": "TsErr Problems"
        }
      ]
    }
  }
}